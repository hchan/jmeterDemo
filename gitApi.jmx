          <JSR223PostProcessor guiclass="TestBeanGUI" testclass="JSR223PostProcessor" testname="cache the items and begin an index loop count" enabled="true">
            <stringProp name="cacheKey"></stringProp>
            <stringProp name="filename"></stringProp>
            <stringProp name="parameters"></stringProp>
            <stringProp name="script">var response = eval(&apos;(&apos; + prev.getResponseDataAsString() + &apos;)&apos;);
//OUT.println(JSON.stringify(response, null, 1));
OUT.println(&quot;number of items (aka as total_count): &quot; + response.items.length)

var endOfItems = false;
var indexOfItem = 0;

if (indexOfItem &lt; response.items.length) {
	endOfItems = false;
} else {
	endOfItems = true;
}
vars.put(&quot;indexOfItem&quot;, 0);
vars.put(&quot;items&quot;, JSON.stringify(response.items));
vars.put(&quot;endOfItems&quot;, endOfItems);</stringProp>
            <stringProp name="scriptLanguage">javascript</stringProp>
          </JSR223PostProcessor>
