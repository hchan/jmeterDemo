          <JSR223Sampler guiclass="TestBeanGUI" testclass="JSR223Sampler" testname="increment each loop count" enabled="true">
            <stringProp name="cacheKey"></stringProp>
            <stringProp name="filename"></stringProp>
            <stringProp name="parameters"></stringProp>
            <stringProp name="script">var indexOfItem = vars.get(&quot;indexOfItem&quot;);
var items = vars.get(&quot;items&quot;);
var endOfItems = vars.get(&quot;endOfItems&quot;);

items = eval(&apos;(&apos; + items + &apos;)&apos;);
if (indexOfItem &lt; items.length-1) {
	endOfItems = false;
} else {
	endOfItems = true;
}

vars.put(&quot;indexOfItem&quot;, ++indexOfItem);
vars.put(&quot;endOfItems&quot;, endOfItems);
vars.put(&quot;user&quot;, items[indexOfItem].owner.login);
OUT.println(&quot;index:&quot; + indexOfItem + &quot;, &quot; + vars.get(&quot;user&quot;));</stringProp>
            <stringProp name="scriptLanguage">javascript</stringProp>
          </JSR223Sampler>
